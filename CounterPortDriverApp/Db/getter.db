## getter.db

record(ao, "$(USER):retrieval")
{
    field(DTYP, "asynFloat64")
    field(OUT, "@asyn($(PORT),$(ADDR),$(TIMEOUT))SCOPE_GETTER")
    ##field(SCAN, "I/O Intr")
  }

record(mbbi, "$(USER):HXR_STATE")
{
  field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))HXR_STATE")
  field(DTYP, "asynInt32")
  field(ZRVL, "0")
  field(ONVL, "1")
  field(TWVL, "2")
  field(THVL, "3")
  field(FRVL, "4")
  field(FVVL, "5")
  field(SXVL, "6")
  field(ZRST, "Beam Off")
  field(ONST, "Inj. Spectrometer")
  field(TWST, "TD-11")
  field(THST, "DT")
  field(FRST, "BYKIK")
  field(FVST, "TDUND")
  field(SXST, "HXR Dump")
  field(SCAN, "I/O Intr")
}

